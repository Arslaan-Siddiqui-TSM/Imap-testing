# Gmail OAuth
GMAIL_CLIENT_ID=<your-gmail-client-id>
GMAIL_CLIENT_SECRET=<your-gmail-client-secret>
GMAIL_ACCESS_TOKEN=<short-lived-access-token>
GMAIL_REFRESH_TOKEN=<long-lived-refresh-token>

# Outlook OAuth
OUTLOOK_CLIENT_ID=<your-outlook-client-id>
OUTLOOK_TENANT_ID=<your-outlook-tenant-id> or common
OUTLOOK_ACCESS_TOKEN=<generated-outlook-access-token>